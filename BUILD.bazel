genrule(
    name = "buildinfo_gen",
    outs = ["buildinfo.txt"],
    cmd = "grep STABLE_ bazel-out/stable-status.txt > $@",
    stamp = 1,
    visibility = ["//visibility:public"],
)

alias(
    name = "tsconfig.json",
    actual = "//ts:tsconfig.json",
    visibility = ["//visibility:public"],
)
